{
  "version": 2,
  "name": "gori-inmobiliaria",
  "builds": [
    { "use": "@vercel/node", "src": "./public" },
    {
      "src": "./public",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public"
    }
  ]
}
